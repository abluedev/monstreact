{
	"$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnusedVariables": {
					"level": "error",
					"fix": "none"
				}
			},
			"a11y": {
				"noNoninteractiveElementToInteractiveRole": {
					"level": "error",
					"fix": "none"

				}
			},
			"complexity": {
				"noBannedTypes": {
					"level": "error",
					"fix": "safe"
				},
				"noExcessiveCognitiveComplexity": {
					"level": "error",
					"options": {
						"maxAllowedComplexity": 15
					}
				}
			}
		}
	}
	}
